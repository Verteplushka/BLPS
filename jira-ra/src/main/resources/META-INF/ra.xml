<connector xmlns="https://jakarta.ee/xml/ns/jca"
           version="2.0">
    <display-name>Jira RA</display-name>
    <vendor-name>MyTeam</vendor-name>
    <eis-type>Jira</eis-type>
    <resourceadapter-version>1.0</resourceadapter-version>
    <resourceadapter>
        <outbound-resourceadapter>
            <connection-definition>
                <managedconnectionfactory-class>ru.myjira.jca.JiraManagedConnectionFactory</managedconnectionfactory-class>
                <connectionfactory-interface>ru.myjira.jca.JiraConnectionFactory</connectionfactory-interface>
                <connectionfactory-impl-class>ru.myjira.jca.JiraConnectionFactory</connectionfactory-impl-class>
                <connection-interface>ru.myjira.jca.JiraConnection</connection-interface>
                <connection-impl-class>ru.myjira.jca.impl.JiraConnectionImpl</connection-impl-class>
            </connection-definition>
        </outbound-resourceadapter>
    </resourceadapter>
</connector>